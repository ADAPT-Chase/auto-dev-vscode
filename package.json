{
  "name": "auto-dev-vscode",
  "displayName": "auto-dev-vscode",
  "description": "Put the most loved ChatGPT into your vscode, and have things done quickly",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:csharp",
    "onLanguage:go",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:kotlin",
    "onLanguage:typescript",
    "onLanguage:python",
    "onLanguage:rust",
    "onLanguage:swift",
    "onLanguage:zig",
    "onLanguage:lua"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "auto-dev-vscode.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "rebuild": "electron-rebuild -f -b",
    "preinstall": "npx only-allow pnpm",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "electron": "19.1",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "node-loader": "^2.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "tree-sitter": "^0.20.1",
    "tree-sitter-c": "^0.20.2",
    "tree-sitter-c-sharp": "^0.20.0",
    "tree-sitter-cpp": "^0.20.0",
    "tree-sitter-go": "^0.19.1",
    "tree-sitter-java": "^0.19.1",
    "tree-sitter-javascript": "^0.19.0",
    "tree-sitter-kotlin": "^0.2.11",
    "tree-sitter-lua": "^2.1.3",
    "tree-sitter-python": "^0.20.1",
    "tree-sitter-rust": "^0.20.3",
    "tree-sitter-swift": "^0.3.6",
    "tree-sitter-typescript": "^0.20.1",
    "tree-sitter-zig": "^0.2.0"
  }
}
